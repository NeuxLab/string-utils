{
  "name": "string-utils",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Some utils for string processing",
  "keywords": [],
  "repository": "https://github.com/cc/string-utils",
  "license": "MIT",
  "engines": {
    "cc": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "actionProviders": [
    {
      "name": "base64",
      "description": "Turn string into base64.",
      "input": "text",
      "output": "text",
      "methodName": "base64"
    },
    {
      "name": "capitalize",
      "description": "Converts the first character of string to upper case and the remaining to lower case.",
      "input": "text",
      "output": "text",
      "methodName": "capitalize"
    }
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.85",
    "@types/node": "^8.0.47"
  }
}
